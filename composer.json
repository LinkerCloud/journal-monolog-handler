{
    "name": "linker/journal-monolog-handler",
    "description": "Monolog handler for systemd-journal",
    "keywords": ["log", "logging", "psr-3"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "psr/log": "^1.1",
        "monolog/monolog": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {"Linker\\": "src/Linker"}
    },
    "autoload-dev": {
        "psr-4": {"Linker\\": "tests/Linker"}
    },
    "provide": {
        "psr/log-implementation": "^1.1"
    },
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
